<ul class="nav nav-tabs">
  <li role="presentation"><%= link_to 'Institutions', institutehandlers_path %></li>
  <li role="presentation" class="active"><%= link_to 'Institution based listings', handlers_path %></li>
</ul>
<p id="notice"><%= notice %></p>
<div class="page-header"><h2>Listing Allowedvalues for course level</h2> <%= link_to 'Add allowed values', new_allowedvalueslevel_path %></div>


<table class="table" style="width:300px">
  <thead>
    <tr>
      <th>Values</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @allowedvalueslevels.each do |allowedvalueslevel| %>
      <tr>
        <td><%= allowedvalueslevel.values %></td>
        <td><%= link_to 'Show', allowedvalueslevel %>
       ||<%= link_to 'Edit', edit_allowedvalueslevel_path(allowedvalueslevel) %>||
       <%= link_to 'Destroy', allowedvalueslevel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>





</script>